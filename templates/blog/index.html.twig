{% extends 'base.html.twig' %}

{% block title %}Homepage{% endblock %}

{% block body %}
    <h1>TEMPORAIRE</h1>
    {% for a in articles %}
        <h1>{{ a.getTitre() }}</h1>
        <p>{{ a.getContent() }}</p>
        <h6>{{ a.getPublished().format('Y-m-d H:i:s') }}</h6>
    {% endfor %}
{% endblock %}
